 <%= render 'books/list', users: @users %>
<table class="table table-striped">
 <div class="col-lg-8" >
    <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
    </thead>
   <tbody>

   <% @users.each do |user| %>
<tr>
    <td><%= attachment_image_tag user, :image, class: "img-circle pull-left profile-img", fallback: "no_image.jpg",size: '50x50' %></td>
    <td><%= user.name %></td>
    <td><%= link_to "show", user_path(user.id), class: "user_#{user.id}" %></td>
</tr>

    <% end %>
   </tbody>
 </div>
   </table>
